<div class="flex flex--row">
  {% for flower in plants %}
    <div class="flower">
      <h2 class="text--black">{{ flower.nickname }}</h2>
      <img
        class="image-dither"
        src="{{ flower.plant_thumb_path }}"
        alt="{{ flower.name }}"
        style="aspect-ratio: 1/1; object-fit: cover;"
      >
    </div>
  {% endfor %}
</div>
